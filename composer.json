{
    "name": "code-transform/type-inference",
    "description": "Applies introspective transformations on source code",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/microsoft/tolerant-php-parser"
        },
        {
            "type": "vcs",
            "url": "https://github.com/phpactor/worse-reflection"
        }
    ],
    "require": {
        "microsoft/tolerant-php-parser": "dev-master",
        "phpactor/worse-reflection": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\CodeTransform\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\CodeTransform\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
